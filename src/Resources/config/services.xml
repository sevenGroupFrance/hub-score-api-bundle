<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" autoconfigure="true"/>
        <service id="seven_group_france.hub_score_api.api_call" class="SevenGroupFrance\HubScoreApiBundle\ApiCall" autowire="true">
            <argument></argument>
            <argument></argument>
        </service>
        <service id="SevenGroupFrance\HubScoreApiBundle\EventSubscriber\HubScoreApi" autowire="true">
            <tag name="doctrine.event_subscriber" priority="500" connection="default"/>
        </service>
    </services>
</container>
